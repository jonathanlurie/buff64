{
  "name": "buff64",
  "version": "0.1.1",
  "description": "A typed array to base64 codec for Node and Browsers",
  "repository": "jonathanlurie/buff64",
  "moduleName": "buff64",
  "entry": "src/main.js",
  "main": "dist/buff64.cjs.js",
  "commonjs": "dist/buff64.cjs.js",
  "module": "dist/buff64.module.js",
  "browser": "dist/buff64.umd.js",
  "scripts": {
